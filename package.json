{
  "name": "front",
  "version": "0.1.0",
  "description": "FRONT - Plateforme associative",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://git.dev.juniorisep.com/iseplife/front"
  },
  "scripts": {
    "start": "yarn run tailwind:css && react-app-rewired start",
    "start:prod": "yarn run tailwind:css && cross-env GENERATE_SOURCEMAP=false REACT_APP_HTTP_PROTOCOL=https REACT_APP_WS_PROTOCOL=wss REACT_APP_API_URL=dev.iseplife.fr/api react-app-rewired start",
    "build": "yarn run tailwind:css && react-app-rewired build",
    "build:prod": "yarn run tailwind:css && cross-env GENERATE_SOURCEMAP=false CI=false REACT_APP_HTTP_PROTOCOL=https REACT_APP_WS_PROTOCOL=wss REACT_APP_API_URL=dev.iseplife.fr/api react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "tailwind:css": "tailwind build public/css/tailwind.css -c tailwind.config.js -o public/css/index.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.0.0",
    "@ant-design/icons-svg": "^4.1.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "antd": "^4.4.1",
    "axios": "^0.21.0",
    "babel-plugin-import": "^1.13.0",
    "date-fns": "^2.8.1",
    "formik": "^2.0.7",
    "i18next": "^19.0.2",
    "leaflet": "^1.7.1",
    "lightbox-react": "^0.3.8",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.5",
    "react-big-calendar": "^0.26.0",
    "react-calendar": "^2.19.2",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.2.6",
    "react-leaflet": "^2.6.1",
    "react-photo-gallery": "^8.0.0",
